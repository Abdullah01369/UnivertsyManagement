                <div class="agile-row">
                    <h3>Öğrenci</h3>
                    <div class="login-agileits-top">
                        <form action="#" method="post">

                            <input id="ogrno" type="text" class="email" name="email" Placeholder="Email" required="Boş geçilemez" />

                            <input id="ogrpassword" type="password" class="password" name="password" Placeholder="Password" required="Boş Geçilemez" />

                            @*<div class="g-recaptcha" data-sitekey="6Ld0W6AnAAAAAJ3xtY6cdUk-3BqwtYB5tBw0QJ45"></div>*@
