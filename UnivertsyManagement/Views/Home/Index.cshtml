    <!--popup-js-->
    <script src="~/Content/weblogin/js/jquery.magnific-popup.js" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            $('.popup-with-zoom-anim').magnificPopup({
                type: 'inline',
                fixedContentPos: false,
                fixedBgPos: true,
                overflowY: 'auto',
                closeBtnInside: true,
                preloader: false,
                midClick: true,
                removalDelay: 300,
                mainClass: 'my-mfp-zoom-in'
            });

        });
    </script>
    <!--//popup-js-->
    <script>

        function LoginStudent() {

    var student = {
        Student_No: $("#ogrno").val(),
        Password: $("#ogrpassword").val()
    };

    $.ajax({
        type: "POST",
        url: "@Url.Action("LoginStudent", "Login")",
        data: JSON.stringify(student),
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        success: function (response) {
            if (response == "1") {
                alert("Giriş Başarılı");
                debugger;

                window.location.href="/Student/Index"
            }
            if (response == "0") {
                debugger;
                alert("Şifre ya da numara yanlış");

            }
        },
        error: function () {
            alert("Hata!");
        }
    });
}
    </script>
    <script>

        function AcademicianLogin() {

    var academician = {
        AcademicianMail: $("#AcademicianMail").val(),
        AcademicianPassword: $("#AcademicianPassword").val()
    };

    $.ajax({
        type: "POST",
        url: "@Url.Action("LoginAcademician", "Login")",
        data: JSON.stringify(academician),
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        success: function (response) {
            if (response == "1") {
                alert("Giriş Başarılı");
                debugger;

                window.location.href="/Academician/Index"
            }
            if (response == "0") {
                debugger;
                alert("Şifre ya da mail yanlış");

            }
        },
        error: function () {
            alert("Hata!");
        }
    });
}
    </script>

